{
  "cwlVersion": "v1.1",
  "class": "CommandLineTool",
  "requirements": {
    "InlineJavascriptRequirement": {},
    "DockerRequirement": {
      "dockerImageId": "example2:latest"
    },
    "ShellCommandRequirement": {},
    "InitialWorkDirRequirement": {
      "listing": []
    }
  },
  "inputs": {
    "f": {
      "type": "File",
      "inputBinding": {
        "position": 1
      }
    }
  },
  "outputs": {
    "data_output": {
      "type": "File",
      "outputBinding": {
        "glob": "cleaned_data.csv"
      }
    }
  },
  "arguments": [
    {
      "shellQuote": false,
      "valueFrom": "ln -s /code/* ./\npython clean_data.py"
    }
  ]
}